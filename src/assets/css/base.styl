html, body
  height 100%
  min-width 1200px
  margin 0
  padding 0
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  font-family: Helvetica Neue, Helvetica, PingFang SC, Hiragino Sans GB,Microsoft YaHei, Arial, sans-serif;
  font-family: "PingFangSC-Regular";
ol,ul
  list-style none
h1, h2, h3, h4, h5, h6, p, ul
  font-size 100%
  padding 0
  margin 0
body,html,#app
  height 100%
.left
  float left
.right
  float right
a
  text-decoration none
img
  vertical-align middle

.empty
  width: 100%
  text-align: center
  font-size: 14px

.clearfix
  clear: both

.m-t-o-4
  margin-top: -4px !important
.m-l-10
  margin-left: 10px !important

.key-btn {
  width: 28px;
  height: 22px;
  font-size: 14px !important;
  font-weight: 400;
  color: rgba(24, 144, 255, 1);
  line-height: 22px;
  margin-left: 10px;
}


.content-container
  .add-btn
    position absolute
    top 10px
    right 30px
  .card-content
    margin 24px
    line-height 32px
    min-height 700px
    position relative
    .content-input
      margin-bottom 24px
      .input-layout
        min-width 330px
      .lable
        width 70px
        font-size 14px
        text-align right
        margin-right 10px
    .page
      position absolute
      bottom 30px
      right 200px
  .content-input
    .el-input__inner
      width 224px
  .el-card__body
    padding 24px 32px 90px
    .el-date-editor .el-range-separator, .el-date-editor .el-range__icon
      line-height 24px
  .el-card__header
    height: 56px
    line-height: 24px
    font-size: 16px
    font-weight: 500
    padding: 16px 32px
    color: rgba(0, 0, 0, 0.85)
    border-bottom: 1px solid rgba(233, 233, 233, 1)

// 详情页tab栏
.detail-nav-tab
  .el-tabs__header
    margin: 0;
  .el-tabs__item
    font-size: 14px;
    font-weight: 400;
    color: rgba(0, 0, 0, 0.65);
  .el-tabs__item.is-active
    color: rgba(24, 144, 255, 1);
  .el-tabs__nav-wrap::after
    height: 0;

.card-box
  margin: 24px;
  min-height: 630px;
  position: relative;
.a-empty
  color #1890ff
  cursor pointer
.info-content
  .info-table
    width: 100%
    font-size: 14px
    border-collapse: collapse
    margin-bottom: 10px
    table-layout: fixed
    tr
      width: 100%
      height: 73px
      line-height: 73px
    td
      word-break break-all
      word-wrap:break-word
      line-height: 20px
    .table-lable
      width: 11.36%
      text-align: center
      background: rgba(250, 250, 250, 1)
      border: 1px solid rgba(233, 233, 233, 1)
      color: rgba(0, 0, 0, 0.85)

    .table-content
      width: 21.27%
      height: 73px
      padding-left: 25px
      color: rgba(0, 0, 0, 0.65)
      background: rgba(255, 255, 255, 1)
      border: 1px solid rgba(233, 233, 233, 1)
      .colspan
        width: 88.64%

.el-message,el-message--warning
  z-index 3000!important
    